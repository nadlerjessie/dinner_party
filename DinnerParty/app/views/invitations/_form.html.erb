<%= form_tag dinner_invitations_path(@dinner), method: "post" do %>
  <% @available_users.each do |user| %>
    <p>
      <%= check_box_tag("[invitation][user_ids][]", user.id) %>
      <%= label_tag (:user_ids), user.name %>
    </p>
    <% end %>
  <%= submit_tag("Invite Guests", :class => "btn btn-default")%>
<% end %> 